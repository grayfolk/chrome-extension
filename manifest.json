{
  "name": "Chrome Extension",
  "description": "Chrome Extension",
  "version": "0.3",
  "homepage_url": "https://chrome.grayfolk.net",
  "incognito": "split",
  "permissions": ["storage","https://chrome.grayfolk.net/api*"],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2
}
